            "accordion_row_PqR901": {
              "type": "_accordion-row",
              "name": "t:names.accordion_row",
              "settings": {
                "heading": "Are TULIPS adult diapers disposable or reusable?",
                "open_by_default": false,
                "hide_if_empty": false,
                "icon": "none",
                "width": 20
              },
              "blocks": {
                "text_StU234": {
                  "type": "text",
                  "name": "t:names.text",
                  "settings": {
                    "text": "<p>These are single-use diapers. The user can urinate multiple times depending on absorbency capacity.</p><p><strong>After use:</strong></p><ul><li>Wrap the diaper properly.</li><li>Dispose of it in a dry waste bin.</li><li>Do not flush adult diapers in the toilet.</li></ul>",
                    "width": "100%",
                    "max_width": "normal",
