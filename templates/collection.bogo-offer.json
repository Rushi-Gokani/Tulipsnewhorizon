    "section_WnwHCT": {
      "type": "section",
      "blocks": {
        "text_JpGAEq": {
          "type": "text",
          "name": "t:names.heading",
          "settings": {
            "text": "<h3>Frequently asked questions</h3>",
            "width": "fit-content",
            "max_width": "narrow",
